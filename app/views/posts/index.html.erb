<div class="all-posts">
<% @posts.each do |post| %>

<div class ="actual-post">
<%= render partial: 'votes', locals: {post: post} %>
  <span><%= post.id%>. </span><a href="<%= post.url %>"><%= post.title %></a><br>
  <span>By:<%= post.author.username %> <%= time_since_posted(post)%> hours ago | <%= link_to "#{post.comments.size} comments" , post_path(post) %></span>
  </div>
<% end %>
</div>
